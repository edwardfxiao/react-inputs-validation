    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-rewire": "^1.2.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.2.0",
    "core-js": "^2.5.7",
